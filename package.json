{
  "name": "easy-settings",
  "version": "0.0.4",
  "description": "Simple cascading configuration store.",
  "author": "TODO",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel lib/ -d dist/ --source-maps",
    "eslint": "eslint src",
    "test": "istanbul cover node_modules/.bin/_mocha -- -u exports --compilers js:babel-register -R spec -r test/setup.js test/*{,*/}*.js",
