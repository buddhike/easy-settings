    "prepublish": "npm run eslint && npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-async-to-generator": "^6.7.4",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "eslint": "^2.9.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.2",
    "parallelshell": "^2.0.0",
    "proxyquire": "^1.7.4",
    "sinon": "^1.17.4"
  }
}
